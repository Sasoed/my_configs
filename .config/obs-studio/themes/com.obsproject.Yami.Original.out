


/* --------------------- */
/* General Styling Hints */

/* Backgrounds */

.bg_window {
    background-color: #1d1f26;
}

.bg-base {
    background-color: #272a33;
}

.text-heading {
    font-size: 25pt;
    font-weight: bold;
}

.text-large {
    font-size: 11pt;
}

.text-bright {
    color: #476bd7;
}

.text-muted {
    color: #999999;
}

.text-warning {
    color: #b88a16;
}

.text-danger {
    color: #c01c37;
}

.text-success {
    color: #25a231;
}

.frame-notice {
    background: #13141a;
    border-radius: 4px;
    padding: 7px 4px;
}

.frame-notice QLabel {
    padding: 4px 0px;
}

/* Icon Overrides */

.icon-plus {
    qproperty-icon: url(theme:Dark/plus.svg);
}

.icon-minus {
    qproperty-icon: url(theme:Dark/minus.svg);
}

.icon-trash {
    qproperty-icon: url(theme:Dark/trash.svg);
}

.icon-clear {
    qproperty-icon: url(theme:Dark/entry-clear.svg);
}

.icon-gear {
    qproperty-icon: url(theme:Dark/settings/general.svg);
}

.icon-dots-vert {
    qproperty-icon: url(theme:Dark/dots-vert.svg);
}

.icon-refresh {
    qproperty-icon: url(theme:Dark/refresh.svg);
}

.icon-cogs {
    qproperty-icon: url(theme:Dark/cogs.svg);
}

.icon-touch {
    qproperty-icon: url(theme:Dark/interact.svg);
}

.icon-up {
    qproperty-icon: url(theme:Dark/up.svg);
}

.icon-down {
    qproperty-icon: url(theme:Dark/down.svg);
}

.icon-pause {
    qproperty-icon: url(theme:Dark/media-pause.svg);
}

.icon-filter {
    qproperty-icon: url(theme:Dark/filter.svg);
}

.icon-revert {
    qproperty-icon: url(theme:Dark/revert.svg);
}

.icon-save {
    qproperty-icon: url(theme:Dark/save.svg);
}

.icon-close {
    qproperty-icon: url(theme:Dark/close.svg);
}

/* Media icons */

.icon-media-play {
    qproperty-icon: url(theme:Dark/media/media_play.svg);
}

.icon-media-pause {
    qproperty-icon: url(theme:Dark/media/media_pause.svg);
}

.icon-media-restart {
    qproperty-icon: url(theme:Dark/media/media_restart.svg);
}

.icon-media-stop {
    qproperty-icon: url(theme:Dark/media/media_stop.svg);
}

.icon-media-next {
    qproperty-icon: url(theme:Dark/media/media_next.svg);
}

.icon-media-prev {
    qproperty-icon: url(theme:Dark/media/media_previous.svg);
}

/* Default widget style, we override only what is needed. */

QWidget {
    alternate-background-color: #272a33;
    color: #ffffff;
    selection-background-color: #284cb8;
    selection-color: #ffffff;
    font-size: 10pt;
    font-family: 'Open Sans', '.AppleSystemUIFont', Helvetica, Arial, 'MS Shell Dlg', sans-serif;
}

QWidget:disabled {
    color: #999999;
}

/* Container windows */

QDialog,
QMainWindow,
QStatusBar,
QMenuBar,
QMenu {
    background-color: #1d1f26;
}

/* macOS Separator Fix */

QMainWindow::separator {
    background: transparent;
    width: 4px;
    height: 4px;
    margin: 0px;
}

QMainWindow::separator:hover {
    border: 1px solid #ffffff;
    margin: 1px;
}

/* General Widgets */

QLabel,
QGroupBox,
QCheckBox {
    background: transparent;
}

QComboBox,
QCheckBox,
QPushButton,
QSpinBox,
QDoubleSpinBox {
    margin-top: 1.600000px;
    margin-bottom: 1.600000px;
}

QListView QWidget,
QListWidget QWidget {
    margin-top: 0;
    margin-bottom: 0;
}

* [frameShape="1"],
* [frameShape="2"],
* [frameShape="3"],
* [frameShape="4"],
* [frameShape="5"],
* [frameShape="6"] {
    border: 1px solid #272a33;
}

* {
    spacing: 1px;
}

/* Misc */

QAbstractItemView {
    background-color: #272a33;
}

QToolTip {
    background-color: #272a33;
    color: #ffffff;
    border: none;
}

/* Context Menu */

QMenu::indicator {
    left: 4px;
}

QMenu::icon {
    left: 7px;
}

QMenu::separator {
    background: #3c404d;
    height: 1px;
    margin: 1.600000px 4px;
}

QMenu::item:disabled {
    color: #999999;
    background: transparent;
}

QMenu::right-arrow {
    image: url(theme:Dark/expand.svg);
}

/* Top Menu Bar Items */
QMenuBar::item {
    background-color: transparent;
    padding: 4px 7px;
}

QMenuBar::item:selected {
    background: #284cb8;
}

/* Item Lists */
QListWidget {
    border-radius: 4px;
}

QListWidget::item {
    color: #ffffff;
}

QListView,
QListWidget,
QMenu {
    padding: 1.600000px;
}

QMenu {
    border: 1px solid #3c404d;
}

QListWidget::item,
SourceTreeItem {
    padding: 4px 4px;
}

QMenu::item {
    padding: 4px 12px;
    padding-right: 20px;
}

QListView::item,
QListWidget::item,
QMenu::item {
    border-radius: 4px;
    color: #ffffff;
    border: 1px solid transparent;
}

SourceTreeItem {
    border-radius: 4px;
    color: #ffffff;
}

SourceTreeItem .checkbox-icon {
    margin-right: 0;
    margin-left: 4px;
}

SourceTree .indicator-expand {
    margin-right: 4px;
    margin-left: 0;
}

QMenu::item:selected,
QListView::item:selected,
QListWidget::item:selected {
    background-color: #284cb8;
}

QMenu::item:hover,
QListView::item:hover,
QListWidget::item:hover,
QMenu::item:selected:hover,
QListView::item:selected:hover,
QListWidget::item:selected:hover {
    background-color: #476bd7;
    color: #ffffff;
}

QMenu::item:focus,
QListView::item:focus,
QListWidget::item:focus,
QMenu::item:selected:focus,
QListView::item:selected:focus,
QListWidget::item:selected:focus {
    border: 1px solid transparent;
}

QListView::item:disabled,
QListView::item:disabled:hover,
QListWidget::item:disabled,
QListWidget::item:disabled:hover {
    background: transparent;
    color: #999999;
}

QListView QLineEdit,
QListWidget QLineEdit {
    padding: 0;
    padding-bottom: 1px;
    margin: 0;
    border: 1px solid #ffffff;
    border-radius: 4px;
}

QListView QLineEdit:focus,
QListWidget QLineEdit:focus {
    border: 1px solid #5b6273;
}

/* Settings QList */

OBSBasicSettings QScrollBar:vertical {
    width: 21px;
    margin-left: 9px;
}

OBSBasicSettings QScrollBar:horizontal {
    height: 21px;
    margin-top: 9px;
}

/* Settings properties view */
OBSBasicSettings #PropertiesContainer {
    background-color: #272a33;
}

/* Dock Widget */
OBSDock > QWidget {
    background: #272a33;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    border: 1px solid #3c404d;
    border-top: none;
}

#transitionsFrame {
    padding: 4px;
}

OBSDock QLabel {
    background: transparent;
}

QDockWidget {
    font-size: 10pt;
    font-weight: bold;

    titlebar-close-icon: url(theme:Dark/close.svg);
    titlebar-normal-icon: url(theme:Dark/popout.svg);
}

QDockWidget::title {
    text-align: left;
    background-color: #3c404d;
    padding: 4px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QDockWidget::close-button,
QDockWidget::float-button {
    border: none;
    border-radius: 4px;
    background: transparent;
    margin-right: 1px;
    min-width: 16px;
    min-height: 16px;
    icon-size: 16px;
    padding: 2px;
}

QDockWidget::close-button:hover,
QDockWidget::float-button:hover {
    background: #464b59;
}

QDockWidget::close-button:pressed,
QDockWidget::float-button:pressed {
    background: #1d1f26;
    padding: 1px -1px -1px 1px;
}

QScrollArea {
    border-radius: 4px;
}

/* Qt enforces a padding inside its status bar, so we
 * oversize it and use margin to crunch it back down
 */
OBSBasicStatusBar {
    margin-top: 4px;
    border-top: 1px solid #3c404d;
    background: #272a33;
}

StatusBarWidget > QFrame {
    border: 0px solid #3c404d;
    border-left-width: 1px;
    padding: 0px 7px 1px;
}

/* Group Box */

QGroupBox {
    background: #272a33;
    border-radius: 4px;
    padding-top: 30;
    padding-bottom: 4px;
    font-weight: bold;
    margin-bottom: 4px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 4px;
    top: 4px;
}


/* ScrollBars */

QScrollBar {
    background-color: #272a33;
    margin: 0px;
    border-radius: 4px;
}

::corner {
    background-color: #1d1f26;
    border: none;
}

QScrollBar:vertical {
    width: 12px;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical,
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    border: none;
    background: none;
    color: none;
}

QScrollBar:horizontal {
    height: 12px;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
    width: 0px;
}

QScrollBar::left-arrow:horizontal,
QScrollBar::right-arrow:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    border: none;
    background: none;
    color: none;
}

QScrollBar::handle {
    background-color: #3c404d;
    margin: 2px;
    border-radius: 2px;
    border: 1px solid #3c404d;
}

QScrollBar::handle:hover {
    background-color: #464b59;
    border-color: #4e5566;
}

QScrollBar::handle:pressed {
    background-color: #13141a;
    border-color: #13141a;
}

QScrollBar::handle:vertical {
    min-height: 32px;
}

QScrollBar::handle:horizontal {
    min-width: 32px;
}

QScrollBar::handle:disabled {
    background: transparent;
    border-color: transparent;
}

/* Source Context Bar */

#contextContainer {
    background-color: #272a33;
    margin-top: 4px;
    border-radius: 4px;
}

#contextContainer QPushButton {
    padding-left: 12px;
    padding-right: 12px;
}

QPushButton#sourcePropertiesButton {
    qproperty-icon: url(theme:Dark/settings/general.svg);
    icon-size: 16px;
}

QPushButton#sourceFiltersButton {
    qproperty-icon: url(theme:Dark/filter.svg);
    icon-size: 16px;
}

/* Scenes and Sources toolbar */

QToolBar {
    background-color: transparent;
    border: none;
    margin: 1.600000px 0px;
    spacing: 1.600000px;
}

QToolBarExtension {
    background: #3c404d;
    min-width: 12px;
    max-width: 12px;
    padding: 4px 0px;
    margin-left: 0px;

    qproperty-icon: url(theme:Dark/dots-vert.svg);
}


/* Tab Widget */

/* The tab widget frame */
QTabWidget::pane {
    border-top: 4px solid #272a33;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar QToolButton {
    background: #3c404d;
    border: none;
}

QTabBar::tab:top {
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}

QTabBar::tab:bottom {
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}

QTabBar::tab {
    background: #272a33;
    color: #ffffff;
    border: none;
    padding: 8px 12px;
    min-width: 50px;
    margin: 1px 0px;
    margin-right: 2px;
    border: 1px solid #3c404d;
}

QTabBar::tab:pressed {
    background: #284cb8;
}

QTabBar::tab:hover {
    background: #464b59;
    border-color: #5b6273;
    color: #ffffff;
}

QTabBar::tab:focus {
    border-color: #5b6273;
}

QTabBar::tab:selected {
    background: #284cb8;
    color: #ffffff;
}

QTabBar::tab:top {
    border-bottom: 0px solid transparent;
    margin-bottom: 0px;
}

QTabBar::tab:bottom {
    border-top: 0px solid transparent;
    margin-top: 0px;
}

QTabBar QToolButton {
    background: #3c404d;
    min-width: 16px;
    padding: 0px;
}

/* ComboBox */

QComboBox,
QDateTimeEdit {
    background-color: #3c404d;
    border: 1px solid #3c404d;
    border-radius: 4px;
    padding: 4px 8px;
    height: 22px;
}

QComboBox QAbstractItemView {
    padding: 1.600000px 4px;
    border: 1px solid #3c404d;
    outline: none;
}

QComboBox QAbstractItemView::item {
    padding: 4px 7px;
}

QComboBox QAbstractItemView::item:selected,
QComboBox QAbstractItemView::item:hover {
    background-color: #284cb8;
}

QComboBox:hover,
QComboBox:focus,
QDateTimeEdit:hover,
QDateTimeEdit:selected {
    border-color: #5b6273;
}

QComboBox:on {
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
}

QComboBox::drop-down,
QDateTimeEdit::drop-down {
    border: none;
    border-left: 1px solid #272a33;
    width: 22px;
}

QComboBox::down-arrow,
QDateTimeEdit::down-arrow {
    qproperty-alignment: AlignTop;
    image: url(theme:Dark/collapse.svg);
    width: 100%;
}

QComboBox:editable:hover {
    background-color: #1d1f26;
    border-color: #5b6273;
}

QComboBox:on,
QDateTimeEdit:on,
QComboBox:editable:focus {
    background-color: #1d1f26;
    border-color: #284cb8;
}

QComboBox::drop-down:editable,
QDateTimeEdit::drop-down:editable {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

QComboBox::down-arrow:editable,
QDateTimeEdit::down-arrow:editable {
    qproperty-alignment: AlignTop;
    image: url(theme:Dark/collapse.svg);
    width: 100%;
}

/* Textedits etc */

QLineEdit,
QTextEdit,
QPlainTextEdit {
    background-color: #3c404d;
    border: none;
    border-radius: 4px;
    padding: 4px 8px;
    border: 1px solid #3c404d;
    height: 22px;
}

QLineEdit:hover,
QTextEdit:hover,
QPlainTextEdit:hover {
    background-color: #1d1f26;
    border-color: #5b6273;
}

QLineEdit:focus,
QTextEdit:focus,
QPlainTextEdit:focus {
    background-color: #1d1f26;
    border-color: #284cb8;
}

QLineEdit:read-only,
QLineEdit:read-only:hover,
QLineEdit:read-only:focus {
    background-color: transparent;
    border-color: #3c404d;
}

QTextEdit:!editable,
QTextEdit:!editable:hover,
QTextEdit:!editable:focus {
    background-color: #3c404d;
}

/* Spinbox and doubleSpinbox */

QSpinBox,
QDoubleSpinBox {
    background-color: #3c404d;
    border: 1px solid #3c404d;
    border-radius: 4px;
    padding: 4px 8px;
    height: 22px;
    max-height: 22px;
}

QSpinBox:hover,
QDoubleSpinBox:hover {
    background-color: #1d1f26;
    border-color: #5b6273;
}

QSpinBox:focus,
QDoubleSpinBox:focus {
    background-color: #1d1f26;
    border-color: #284cb8;
}

QSpinBox::up-button,
QDoubleSpinBox::up-button {
    subcontrol-origin: padding;
    /* position at the top right corner */
    subcontrol-position: top right;

    width: 22px;
    height: 14px;
    border-left: 1px solid #272a33;
    border-bottom: 1px solid transparent;
    border-radius: 0px;
    border-top-right-radius: 2px;
}

QSpinBox::down-button,
QDoubleSpinBox::down-button {
    subcontrol-origin: padding;
    /* position at the top right corner */
    subcontrol-position: bottom right;

    width: 22px;
    height: 14px;
    border-left: 1px solid #272a33;
    border-top: 1px solid #272a33;
    border-radius: 0px;
    border-bottom-right-radius: 2px;
}

QSpinBox::up-button:hover,
QSpinBox::down-button:hover,
QDoubleSpinBox::up-button:hover,
QDoubleSpinBox::down-button:hover {
    background-color: #464b59;
}

QSpinBox::up-button:pressed,
QSpinBox::down-button:pressed,
QDoubleSpinBox::up-button:pressed,
QDoubleSpinBox::down-button:pressed {
    background-color: #1d1f26;
}

QSpinBox::up-button:disabled,
QSpinBox::up-button:off,
QSpinBox::down-button:disabled,
QSpinBox::down-button:off {
    background-color: #272a33;
}

QDoubleSpinBox::up-button:disabled,
QDoubleSpinBox::up-button:off,
QDoubleSpinBox::down-button:disabled,
QDoubleSpinBox::down-button:off {
    background-color: #272a33;
}

QSpinBox::up-arrow,
QDoubleSpinBox::up-arrow {
    image: url(theme:Dark/up.svg);
    width: 100%;
    margin: 2px;
}

QSpinBox::down-arrow,
QDoubleSpinBox::down-arrow {
    image: url(theme:Dark/down.svg);
    width: 100%;
    padding: 2px;
}

/* Controls Dock */
#controlsFrame {
    padding: 4px;
}

#controlsFrame QPushButton {
    margin: 1.600000px 1px;
}

#streamButton,
#recordButton,
#replayBufferButton,
#broadcastButton {
    padding: 4px;
}

#pauseRecordButton,
#saveReplayButton,
#virtualCamConfigButton {
    padding: 4px 4px;
    width: 22px;
    max-width: 22px;
}

/* Primary Control Button Checked Coloring */
#streamButton:!hover:!pressed.state-active,
#recordButton:!hover:!pressed.state-active,
#pauseRecordButton:!hover:!pressed.state-active,
#replayBufferButton:!hover:!pressed.state-active,
#virtualCamButton:!hover:!pressed.state-active,
#modeSwitch:!hover:!pressed.state-active,
#broadcastButton:!hover:!pressed.state-active {
    background: #284cb8;
}

/* Primary Control Button Hover Coloring */
#streamButton:hover:!pressed.state-active,
#recordButton:hover:!pressed.state-active,
#pauseRecordButton:hover:!pressed.state-active,
#replayBufferButton:!pressed.state-active,
#virtualCamButton:!pressed.state-active,
#modeSwitch:hover:!pressed.state-active,
#broadcastButton:hover:!pressed.state-active {
    background: #476bd7;
    color: #ffffff;
}


/* Buttons */

QPushButton {
    background-color: #3c404d;
    color: #ffffff;
    border: 1px solid #3c404d;
    border-radius: 4px;
    height: 22px;
    max-height: 22px;
    margin-top: 1.600000px;
    margin-bottom: 1.600000px;
    padding: 4px 16px;
    icon-size: 16px;
    outline: none;
}

QPushButton:hover {
    background-color: #464b59;
}

QPushButton:hover,
QPushButton:focus {
    border-color: #5b6273;
}

QPushButton::flat {
    background-color: #3c404d;
}

QPushButton:checked {
    background-color: #284cb8;
    border-color: #476bd7;
}

QPushButton:checked:hover,
QPushButton:checked:focus {
    border-color: #718cdc;
}

QPushButton:pressed,
QPushButton:pressed:hover {
    background-color: #1d1f26;
    border-color: #3c404d;
}

QPushButton:disabled {
    background-color: #272a33;
    border-color: #3c404d;
}

QPushButton::menu-indicator {
    image: url(theme:Dark/down.svg);
    subcontrol-position: right;
    subcontrol-origin: padding;
    width: 25px;
}

QToolButton {
    border: 1px solid #3c404d;
}

QToolButton,
.btn-tool {
    background-color: #3c404d;
    padding: 2px 2px;
    margin: 0px 0px;
    border: 1px solid #3c404d;
    border-radius: 4px;
    icon-size: 16px;
}

QToolButton:last-child,
.btn-tool:last-child {
    margin-right: 0px;
}

QToolButton:hover,
QToolButton:focus,
.btn-tool:hover,
.btn-tool:focus,
.indicator-mute::indicator:hover,
.indicator-mute::indicator:focus {
    border-color: #3c404d;
    background-color: #464b59;
}

QToolButton:pressed,
QToolButton:pressed:hover,
.btn-tool:pressed,
.btn-tool:pressed:hover {
    background-color: #1d1f26;
    border-color: #3c404d;
}

QToolButton:disabled,
.btn-tool:disabled {
    background-color: #272a33;
    border-color: transparent;
}

/* Sliders */

QSlider::groove {
    background-color: #3c404d;
    border: none;
    border-radius: 2px;
}

QSlider::groove:horizontal {
    height: 4px;
}

QSlider::groove:vertical {
    width: 4px;
}

QSlider::sub-page:horizontal {
    background-color: #476bd7;
    border-radius: 2px;
}

QSlider::sub-page:horizontal:disabled {
    background-color: #3c404d;
    border-radius: 2px;
}

QSlider::add-page:horizontal:disabled {
    background-color: #1d1f26;
    border-radius: 2px;
}

QSlider::add-page:vertical {
    background-color: #476bd7;
    border-radius: 2px;
}

QSlider::add-page:vertical:disabled {
    background-color: #3c404d;
    border-radius: 2px;
}

QSlider::sub-page:vertical:disabled {
    background-color: #1d1f26;
    border-radius: 2px;
}

QSlider::handle {
    background-color: #ffffff;
    border-radius: 4px;
}

QSlider::handle:horizontal {
    height: 10px;
    width: 20px;
    /* Handle is placed by default on the contents rect of the groove. Expand outside the groove */
    margin: -3px 0;
}

QSlider::handle:vertical {
    width: 10px;
    height: 20px;
    /* Handle is placed by default on the contents rect of the groove. Expand outside the groove */
    margin: 0 -3px;
}

QSlider::handle:hover {
    background-color: #c2c2c2;
}

QSlider::handle:pressed {
    background-color: #d6d6d6;
}

QSlider::handle:disabled {
    background-color: #adadad;
}

/* Volume Control */

#stackedMixerArea QPushButton {
    width: 16px;
    height: 16px;
    background-color: #3c404d;
    padding: 3px 3px;
    margin: 0px;
    border: 1px solid #3c404d;
    border-radius: 4px;
    icon-size: 16px;
}

/* This is an incredibly cursed but necessary fix */
#stackedMixerArea QPushButton:!hover {
    background-color: #3c404d;
}

#stackedMixerArea QPushButton:hover {
