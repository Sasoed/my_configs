    font-size: 10pt;
    color: #999999;
}

/* Horizontal Mixer */
#hMixerScrollArea VolControl {
    padding: 0px 7px 2px;
    border-bottom: 1px solid #3c404d;
}

#hMixerScrollArea VolControl QSlider {
    margin: 0px 0px 2px;
}

