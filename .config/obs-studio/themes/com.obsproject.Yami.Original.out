}

#stackedMixerArea {
    border: none;
    padding: 0px;
    border-bottom: 1px solid #3c404b;
}

VolControl {
    background: #272a33;
}

VolControl QLabel {
    font-size: 8pt;
    margin: 1px 0px;
}

VolControl #volLabel {
